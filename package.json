{
	"name": "@coreproject-moe/monorepo",
	"private": true,
	"scripts": {
		"build": "nx run-many -t build",
		"build:icons": "nx run @coreproject-moe/icons:build",
		"build:vp": "nx run @coreproject-moe/video-player:build",
		"dev": "nx run-many -t start",
		"dev:icons": "nx run @coreproject-moe/icons:start",
		"dev:vp": "nx run @coreproject-moe/video-player:start",
		"test": "nx run-many -t test",
		"test:icons": "nx run @coreproject-moe/icons:test",
		"test:vp": "nx run @coreproject-moe/video-player:test",
    "format": "prettier . --write",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.9",
		"@nx/eslint": "20.1.2",
		"@nx/jest": "20.1.2",
		"eslint": "^9.15.0",
		"jest": "^29.7.0",
		"nx": "20.1.2",
		"prettier": "^3.3.3"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "npm@10.9.0",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"overrides": {
		"babel-plugin-macros": "^3.1.0"
	}
}
